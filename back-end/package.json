{
  "name": "back-end",
  "version": "1.0.0",
  "description": "n2server의 api-server 기능을 담당하는 back-end 서버 모듈",
  "main": ".eslintrc.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:rm": "rm -rf node_modules",
    "test:i:dev": "npm run test:rm & npm i --legacy-peer-deps",
    "test:i:prod": "npm run test:rm & npm i --legacy-peer-deps --only=production",
    "test:build": "nest build",
    "test:run": "node dist/main"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.3.17",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.0",
    "@typegoose/typegoose": "^9.7.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "firebase-admin": "^10.0.2",
    "handlebars": "^4.7.7",
    "mongodb": "^4.4.1",
    "mongoose": "^6.2.4",
    "nestjs-typegoose": "^7.1.38",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "rxjs": "^7.5.4",
    "solapi": "^5.0.0",
    "@nestjs/swagger": "^5.2.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.2",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "rimraf": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "jsonwebtoken": "^8.5.1",
    "crypto-js": "^4.1.1",
    "@types/crypto-js": "^4.1.1",
    "@nestjs/mapped-types": "^1.0.1",
    "passport-local": "^1.0.0",
    "@types/jest": "^27.4.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
